{
  "ServerOptions": {
    "FrontEndUrl": "http://localhost:5173",
    "Environment": "Development",
    "PageSize": 50
  },
  "ConnectionStrings": {
    "DefaultConnection": "server=host;database=DB_NAME;user=usr;password=pwd;port=3306"
  },
  "DatabaseBinaries": {
    "DumpPath": "/usr/bin/mariadb-dump",
    "MariadbPath": "/usr/bin/mariadb"
  },
  "JwtSettings": {
    "Secret": "clave-secreta-segura-de-32-chars",
    "ExpiresMinutes": 10
  },
  "RateLimitSettings": {
    "Guest": {
      "PermitLimit": 300,
      "WindowSeconds": 60,
      "QueueLimit": 2
    },
    "Authenticated": {
      "PermitLimit": 300,
      "WindowSeconds": 60,
      "QueueLimit": 2
    }
  }
}
